{
    "name": "iiirxs/image-upload-bundle",
    "description": "A Symfony bundle to handle multiple images upload with mapped image collections",
    "type": "symfony-bundle",
    "require": {
        "ext-json": "*",
        "ext-gd": "*",
        "ext-mongo": "*",
        "php": "^7.1.3",
        "symfony/http-foundation": "*",
        "symfony/framework-bundle": "^4.0 || ^5.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/expression-language": "^4.2",
        "symfony/form": "^4.3",
        "doctrine/mongodb-odm-bundle": "^3.4 || ^4.0",
        "symfony/property-access": "*",
        "iiirxs/exception-handler-bundle": "@dev",
        "iiirxs/validation-error-normalizer-bundle": "@dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "spatie/image-optimizer": "^1.1",
        "symfony/browser-kit": "^4.0",
        "symfony/validator": "^4.0"
    },
    "suggest": {
        "symfony/serializer": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nik G",
            "email": "iiirxs@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "IIIRxs\\ImageUploadBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IIIRxs\\ImageUploadBundle\\Tests\\": "tests/"
        },
        "files": [ "tests/Util/utils.php" ]
    }
}
